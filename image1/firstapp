import requests
from flask import Flask, jsonify, request
import json




app = Flask(__name__)
  
@app.route("/api/", methods=["POST"])
def hello():
	
	#data = request.get_json()
	data = request.get_json(force=True) 
	print(data)
	return (jsonify(data))
	
if __name__ == '__main__':
   app.run(debug=True, port=5052, host='0.0.0.0')
